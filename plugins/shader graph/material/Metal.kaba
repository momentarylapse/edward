use edward.*

class NodeMetal extends shader.Node
	func __init__()
		super.__init__("Metal")
		output.add([shader.Type.FLOAT, "metal"])
	func override dependencies() -> string[]
		return ["in:material"]
	func override code_pixel(out c: shader.BuilderContext) -> string
		var t = c.create_out(self, 0)
		return "
	float {{t}} = material.metal;"
