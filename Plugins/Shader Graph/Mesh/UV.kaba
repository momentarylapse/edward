use edward

class NodeUV extends shader.Node
	void __init__()
		super.__init__("UV")
		output.add([shader.TYPE_VEC2, "uv"])
	override const string[] dependencies()
		return ["uv"]
	override const string code_pixel(out shader.BuilderContext c)
		let t = c.create_out(self, 0)
		return "
	vec2 {{t}} = in_uv;"
