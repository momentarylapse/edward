use edward

class NodeMultiply extends ShaderNode
	void __init__()
		super.__init__("Multiply")
		params.add([SHADER_TYPE_COLOR, "a", "#ffffffff", ""])
		params.add([SHADER_TYPE_COLOR, "b", "#ffffffff", ""])
		output.add([SHADER_TYPE_COLOR, "out"])
	override const string code_pixel(out ShaderBuilderContext c)
		let t = c.create_out(self, 0)
		return "
	vec4 {{t}} = {{c.value(self, 0)}} * {{c.value(self, 1)}};"
