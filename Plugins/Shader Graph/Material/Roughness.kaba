use edward

class NodeRoughness extends shader.Node
	void __init__()
		super.__init__("Roughness")
		output.add([shader.TYPE_FLOAT, "roughness"])
	override const string[] dependencies()
		return ["in:material"]
	override const string code_pixel(out shader.BuilderContext c)
		var t = c.create_out(self, 0)
		return "
	float {{t}} = material.roughness;"
