use edward

class NodeSpecular extends ShaderNode
	void __init__()
		super.__init__("Specular")
		output.add([SHADER_TYPE_FLOAT, "specular"])
	override const string[] dependencies()
		return ["material"]
	override const string code_pixel(out ShaderBuilderContext c)
		let t = c.create_out(self, 0)
		return "
	float {{t}} = material.specular;"
